<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.IntellComm" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/theme_btn_bg</item>
        <item name="colorPrimaryVariant">@color/theme_btn_bg</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:windowSplashScreenBackground" tools:targetApi="s">@color/purple_200
        </item>
        <item name="android:windowSplashScreenAnimatedIcon" tools:targetApi="s">@drawable/logo
        </item>
        <item name="android:windowSplashScreenAnimationDuration" tools:targetApi="s">1000</item>
        <item name="android:windowSplashScreenIconBackgroundColor" tools:targetApi="s">
            @color/purple_700
        </item>
        <item name="android:windowSplashScreenBrandingImage" tools:targetApi="s">@drawable/logo
        </item>

    </style>

    <style name="Theme.Home" parent="Theme.IntellComm">
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/intro_bg</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>

    </style>

    <style name="Theme.Bottom_nav" parent="Theme.IntellComm">
        <item name="colorPrimary">@color/white</item>
    </style>

    <style name="ButtonStyle16" parent="Theme.IntellComm">
